{
  "name": "carmarket",
  "private": true,
  "workspaces": ["frontend", "backend"],
  "scripts": {
    "start:frontend": "pnpm --filter frontend start",
    "start:backend": "pnpm --filter backend dev",
    "dev":"node backend/server.js",
    "start:all": "pnpm run start:backend & pnpm run start:frontend",
    "test:frontend": "pnpm --filter frontend test",
    "test:backend": "pnpm --filter backend test",
    "test:all": "pnpm run test:frontend && pnpm run test:backend"
  }
}
